<h3>All Venues</h3>

<% @venues.each do |venue| %>

  <ul>
    <li> <a href="/venues/<%= venue.id %>"><%= venue.name %></a></li>
  </ul>
<% end %>


<h3>Add a new venues</h3>
<form action="/venues/new" method="post">
  <label for="venue_name">Venue Name:</label>
  <input type="text" name="venue_name" placeholder="example: CBGB's">
  <button name="submit">Add Your Venue</button>
</form>
